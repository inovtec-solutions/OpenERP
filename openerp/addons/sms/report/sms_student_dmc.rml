  <story>
	<para style="P1">[[repeatIn(get_students_dmc(data['form']),'r')]]</para> 
		<blockTable colWidths="510" style="Table1">
			 <tr><td><para style="P5"><u>DETAILED MARKS CERTIFICATE</u></para></td></tr> 
			 <tr><td><para style="P5">[[get_exam_type(data['form'])]]</para></td></tr>
			 <tr><td><para style="ParaBlack"><u>[[(r['student_class_status'])]]</u></para></td></tr>
		</blockTable>
		
		<blockTable colWidths="115.0,140.0,115.0,140.0" style="Table1">
		   	 <tr><td><para style="P9"><u>ADMISSION NO:</u></para></td><td><para style="P4">[[(r['cadidate_no'])]]</para></td><td><para style="P4"></para></td><td><para style="P4"></para></td></tr>  
		   	 <tr><td><para style="P9"><u>STUDENT:</u></para></td><td><para style="P4">[[(r['student_name'])]]</para></td><td><para style="P9"><u>FATHER'S NAME:</u></para></td><td><para style="P4">[[(r['father_name'])]]</para></td></tr>
		</blockTable>
		
		<section>
			<blockTable colWidths="510" style="">
		    <tr><td>
		    
			<blockTable colWidths="40.0,110.0,70.0,70.0,80.0,70.0,70.0" style="Table_maintable_header">
			<tr>
		          <td>
		            <para style="P6">S.No</para>
		          </td>
		          <td>
		            <para style="P6">TITLE OF SUBJECT</para>
		          </td>
		          <td>
		            <para style="P7">Theory</para>
		          </td>
		          <td>
		            <para style="P7">Practical</para>
		          </td>
		          
		          <td>
		            <para style="P7">M. OBTAINED</para>
		          </td>
				<td>
		            <para style="P7">T.MARKS</para>
		          </td>
			  <td>
		            <para style="P7">PERCENTAGE</para>
		          </td>
				
		      </tr>
		    </blockTable>
			</td></tr>
			<tr><td>
			
		   	<blockTable colWidths="40.0,110.0,70.0,70.0,80.0,70.0,70.0" style="Table_maintable">
			<para style="P1">[[repeatIn((r['result']),'s')]]</para> 
			<tr>
				<td>
					<para style="P10">[[(s['s_no'])]]</para>
				</td>
				<td>
					<para style="P10">[[(s['title'])]]</para>
				</td>
				<td>
					<para style="P2">[[(s['theory'])]]</para>
				</td>
				
				<td>
